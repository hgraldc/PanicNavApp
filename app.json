{
  "expo": {
    "name": "PanicNav",
    "slug": "panicNav",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/PanicNav_launcher.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/PanicNav_launcher.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We need your location to display the map"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/PanicNav_launcher.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.hgraldc.panicNav",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "fe4545c4-80c2-4310-be86-47b6ced10f7c"
      }
    }
  }
}
